{
  "name": "zazap-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "model:create": "npx sequelize-cli model:generate --name",
    "seed:create": "npx sequelize-cli seed:generate --name"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^6.7.19",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.3",
    "glob": "^11.0.3",
    "jimp": "^0.16.13",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.8.1",
    "web-push": "^3.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
